{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "image": "sdk-52",
        "env": {
          "EXPO_DEBUG": "true"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "sdk-52",
        "env": {
          "EXPO_DEBUG": "true"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "simulator": false,
        "env": {
          "EXPO_DEBUG": "true"
        }
      },
      "android": {
        "image": "sdk-52",
        "env": {
          "EXPO_DEBUG": "true"
        }
      }
    }
  }
}
